<h2>{{name}}</h2>
<hr>
{{#if content.length}}
    {{#each content}}
        <div class='card mt-4'>
            <div class='card-body'>
                <h4>{{titleContent}}</h4>
                <small>Descrição: {{descriptionContent}}</small><br>
                <small>Data de criação: {{data}}</small><br>
            </div>
        </div>
    {{/each}}
{{else}}
    <h6 class='mt-2'>Não há conteúdos registrados</h6>
{{/if}}